{
  "name": "qa_automation_swaglabs",
  "version": "1.0.0",
  "description": "This is an UI automation challenge",
  "main": "index.js",
  "scripts": {
    "report": "ts-node ./src/reporting/report-generator.ts",
    "test:json": "npm test -- --reporter cucumber-json --skip-js-errors --reporter-json-folder=src/reporting/json",
    "test": "testcafe chrome src/test/**/*.spec.ts --env=local --user=user1@example.com  --selector-timeout 10000 -S -s reports/screenshots --skip-js-errors --skip-uncaught-errors --hostname localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisZanella/SwagLabs_UI_TestCafe.git"
  },
  "keywords": [
    "TestCafe",
    "UI"
  ],
  "author": "Luis Zanella",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuisZanella/SwagLabs_UI_TestCafe/issues"
  },
  "homepage": "https://github.com/LuisZanella/SwagLabs_UI_TestCafe#readme",
  "dependencies": {
    "@types/faker": "^5.5.9",
    "faker": "^5.5.3",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "testcafe": "^1.16.1",
    "testcafe-reporter-cucumber-json": "^6.3.0",
    "ts-node": "^10.4.0"
  }
}
